{
    "name": "nunomaduro/php-interminal",
    "description": "PHP Internals discussions in your terminal.",
    "keywords": ["php", "internals", "interminal", "console", "cli"],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/nunomaduro/php-interminal/issues",
        "source": "https://github.com/nunomaduro/php-interminal"
    },
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-dom": "*"
    },
    "require-dev": {
        "dg/rss-php": "^1.5",
        "laravel-zero/framework": "^8.9",
        "mockery/mockery": "^1.4",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/interminal"]
}
